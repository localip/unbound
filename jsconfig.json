{
   "exclude": [
      "node_modules"
   ],
   "compilerOptions": {
      "baseUrl": "./src/",
      "paths": {
         "@structures/*": [
            "structures/*"
         ],
         "@modules/*": [
            "modules/*"
         ],
         "@webpack/*": [
            "modules/webpack/*"
         ],
         "@utilities/*": [
            "modules/utilities/*"
         ],
         "@constants": [
            "modules/constants"
         ],
         "@patcher/*": [
            "modules/patcher/*"
         ],
         "@api/*": [
            "api/*"
         ],
         "@core/*": [
            "core/*"
         ],
         "@data/*": [
            "data/*"
         ],
         "@root/*": [
            "./*"
         ],
      }
   }
}