{
   "name": "unbound",
   "version": "1.0.0",
   "description": "A discord client aimed at giving the user control, stability, a beautiful appearance, and freeing them from the shackles of other client modifications.",
   "keywords": [
      "client modification",
      "client mod",
      "discord",
      "discord client",
      "discord mod",
      "discord plugins",
      "discord themes",
      "themes",
      "plugins"
   ],
   "author": "eternal",
   "license": "See LICENSE",
   "scripts": {
      "build": "swc src -d dist -D --delete-dir-on-start",
      "package": "node scripts/pack.mjs",
      "dev": "nodemon"
   },
   "aliases": {
      "@structures": "dist/client/structures",
      "@entities": "dist/client/structures/entities",
      "@modules": "dist/client/modules",
      "@webpack": "dist/client/modules/webpack",
      "@components": "dist/client/components",
      "@utilities": "dist/common/utilities",
      "@common": "dist/common",
      "@constants": "dist/common/constants",
      "@patcher": "dist/client/modules/patcher",
      "@api": "dist/client/api",
      "@core": "dist/client/core",
      "react": "dist/client/modules/webpack/react",
      "@src": "dist",
      "@root": "dist"
   },
   "devDependencies": {
      "@swc/cli": "^0.1.57",
      "@swc/core": "^1.2.219",
      "@types/lodash": "^4.14.182",
      "@types/node": "^17.0.45",
      "@types/react": "^18.0.15",
      "asar": "^3.1.0",
      "discord-types": "^1.3.26",
      "fs-extra": "^10.1.0",
      "typescript": "^4.7.4"
   }
}